{
  "laravel": {
    "breeze": true,
    "stack": "react",
    "ssr": true,
    "typescript": true
  },
  "uiKit": {
    "packages": [
      "lucide-react",
      "class-variance-authority",
      "clsx",
      "tailwind-merge",
      "sonner"
    ]
  },
  "testing": {
    "packages": [
      "pestphp/pest",
      "pestphp/pest-plugin-laravel",
      "laravel/dusk"
    ],
    "devPackages": [
      "larastan/larastan",
      "laravel/pint"
    ]
  },
  "database": {
    "default": "mysql",
    "options": ["mysql", "postgresql", "sqlite"]
  }
}
